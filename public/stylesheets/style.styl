@import 'nib'

.ng-cloak
	display none

/* **********************************
 *        Main page header
 * *********************************/
main_header_text_color = #234679

.page-header-wrapper
	width 980px
	margin 0 auto 17px
	height 150px
	position relative
	z-index 150
	.header-title-wrapper
		height 67px
		.main-logo
			float left
			margin-top 12px
		.user-header-wrapper
			float right
			.user-quick-menu
				height 32px
				padding 6px 0
				.breadcrumbs
					background-color #4b94c2
					color #fff
					padding 2px
					float right
					margin 0
			.user-menu
				padding-top 9px
				.main-login-trigger
				.register-trigger
				.soc-title
					color main_header_text_color
				span
					color #999
				.main-login-trigger
					padding-left 20px
					background-image url("data:image/png;base64,")
					background-repeat no-repeat
					background-position 0 4px
				.register-trigger
					padding-left 20px
					background-image url("data:image/png;base64,")
					background-repeat no-repeat
					background-position 0 0
				.soc-login-facebook
					display inline-block
					width 25px
					height 25px
					background-image url(/i/f_login.png)
					background-repeat no-repeat
					margin -3px 6px
				.soc-login-vk
					display inline-block
					width 26px
					height 25px
					background-image url(/i/vk_login.png)
					background-repeat no-repeat
					margin -3px 0
	.main-search-wrapper
		height 82px
		border 4px solid #25497c
		border-radius 3px
		background-color #25497c
		position relative
		.search-form
			position relative
			border-radius 2px
			z-index 3
			background linear-gradient(top,rgba(79, 155, 201, 1) 0%,#21437a 100%)
			border-bottom 1px solid #254b8a
			box-shadow 0 1px 0 #193462
			padding 6px 24px
			.search-input
				float left
				width 595px
				margin-bottom 0
				background-color #f4f4f4
				border-left 1px solid #4283ad
				border-right none
				border-top 1px solid #4283ad
				border-bottom 1px solid #4283ad
				border-radius 3px 0 0 3px
				box-shadow inset 0 1px 0 #9fbcce, 0 0 0 transparent, inset 0 -1px 0 #1b3d6d, 0 0 0 transparent
			.categories-choice-trigger
				position relative
				border-radius 0 3px 3px 0
				background-color #f4f4f4
				border-right 1px solid #4283ad
				border-left none
				border-top 1px solid #4283ad
				border-bottom 1px solid #4283ad
				height 34px
				width 230px
				float left
				line-height 34px
				box-shadow inset 0 1px 0 #9fbcce, 0 0 0 transparent, inset 0 -1px 0 #1b3d6d, 0 0 0 transparent
				background-image url("data:image/png;base64,")
				background-position 205px 14px
				background-repeat no-repeat
				cursor pointer
				color #999
				padding-right 30px
				text-align right
				user-select none
			.categories-choice
				position absolute
				background-color #f4f4f4
				border 1px solid #4283ad
				right 117px
				top 33px
				z-index 100
				border-radius 3px
				box-shadow 0 5px 15px rgba(0,0,0,.4)
				.s-categories
					list-style none
					margin 0
					padding 0
				.s-category
					padding 6px 24px
					color #999
				.s-category:hover
					background linear-gradient(top,rgba(79, 155, 201, 1) 0%,#21437a 100%)
					color #fff
					border-radius 2px
					cursor pointer
			.search-trigger
				padding-right 12px
				padding-left 24px
				float right
				background linear-gradient(top,#fdfdfd 0%, #f6f6f6 50%, #ebebeb 51%, #ededed 100%)
				height 33px
				border-radius 2px
				color main_header_text_color
				text-shadow 0 1px 0 #fff
				position relative
				box-shadow 0 1px 1px #183260
				border none
				outline none
				.search-label:before
					content ''
					position absolute
					width 14px
					height 13px
					top 10px
					left 6px
					background-image: url("data:image/png;base64,")
					background-repeat no-repeat
					background-position 0 0
			.search-trigger:active
				box-shadow inset 0 1px 1px #183260
				outline none
		.filters-form
			padding 6px 24px 0
			.header-text
				color rgba(255,255,255,0.8)
				margin-right 12px
/* **********************************
 *             Main page
 * *********************************/
.home-page 
	width 980px
	margin 0 auto
	position relative
.left-column
	float left
	width 228px
	padding-right 21px
	.main-categories-wrapper
		padding 4px
		box-shadow inset 0 3px 18px rgba(0,0,0,.2), inset 0 -3px 18px rgba(0,0,0,.2)
		border-radius 3px
	.main-categories-menu
		border-left 1px solid #e2e2e2
		border-right 1px solid #e2e2e2
		border-top 1px solid #fbfbfb
		border-bottom 1px solid #c6c6c6
		border-radius 2px
	.all-categories
		list-style none
		margin 0
		padding 0
		.all-category
			background linear-gradient(top,#f2f2f2 0%, #e8e8e8 100%)
			border-bottom 1px solid #d6d6d6
			border-top 1px solid #fff
			border-left 1px solid #f6f6f6
			border-right 1px solid #f6f6f6
			.ac-link
				padding 9px 6px 9px 36px
				text-decoration none
				color main_header_text_color
				display block
				background-repeat no-repeat
				outline none
			.ac-link.cat-books
				background-image: url("data:image/png;base64,")
				background-position 9px 10px
			.ac-link.cat-pcs
				background-image: url("data:image/png;base64,")
				background-position 8px 10px
			.ac-link.cat-phones
				background-image: url("data:image/png;base64,")
				background-position 9px 10px
			.ac-link.cat-technics
				background-image: url("data:image/png;base64,")
				background-position 8px 9px
			.ac-link.cat-fasion
				background-image: url("data:image/png;base64,")
				background-position 8px 10px
			.ac-link.cat-cds
				background-image: url("data:image/png;base64,")
				background-position 8px 11px
			.ac-link.cat-home
				background-image: url("data:image/png;base64,")
				background-position 8px 9px
			.ac-link.cat-kids
				background-image: url("data:image/png;base64,")
				background-position 9px 9px
			.ac-link.cat-rest
				background-image: url("data:image/png;base64,")
				background-position 8px 10px
			.ac-link.cat-collect
				background-image: url("data:image/png;base64,")
				background-position 8px 10px
			.ac-link.cat-avto
				background-image: url("data:image/png;base64,")
				background-position 8px 13px
			.ac-link.cat-other
				background-image: url("data:image/png;base64,")
				background-position 8px 11px
		.all-category:active
			background linear-gradient(top,#e1e1e1 0%, #e9e9e9 100%)
			border-top 1px solid #f1f1f1
			border-bottom 1px solid #c6c6c6
			border-left 1px solid #e6e6e6
			border-right 1px solid #e6e6e6
		.all-category.first
			border-top none
			box-shadow inset 0 5px 15px rgba(255,255,255,.8)
		.all-category.last
			border-bottom none
	.adv-header
		font-size 10px
		color #888
		margin-bottom 2px
	.adv-wrapper
		width 205px
		height 165px
		border-radius 2px
		box-shadow 0 1px 5px rgba(0,0,0,0.2), 0 -1px 5px rgba(0,0,0,0.2)
		.adv
			width 205px
			height 165px
	.soc-header
		color main_header_text_color
		font-size 14px
	.vnv-f
	.vnv-vk
		color main_header_text_color
		font-size 14px
		padding-left 28px
		background-repeat no-repeat
		height 26px
		padding-top 9px
		display inline-block
	.vnv-f
		background-image url(/i/f_login.png)
		margin-right 6px
	.vnv-vk
		background-image url(/i/vk_login.png)
.main-content
	float left
	width 752px
	padding 0 2px 24px
	overflow hidden
	.lot-wrapper
		position relative
		float left
		width 172px
		height 215px
		border-radius 3px
		z-index 5
		box-shadow 0 2px 2px rgba(0,0,0,.1)
		margin 0 20px 20px 0
	.lot-image-link
		width 142px
		height 142px
		display block
		margin 0 auto
		margin-top 15px
		margin-bottom 9px
	.lot-image-link.gift:after
		content ''
		position absolute
		width 34px
		height 35px
		top 116px
		right 20px
		background-image url("/i/lots/gift_mark.png")
		background-repeat no-repeat
	.lot-image-link.closehome:after
		content ''
		position absolute
		width 34px
		height 35px
		top 116px
		right 20px
		background-image url("/i/lots/close_mark.png")
		background-repeat no-repeat
	.lot-image
		position relative
		width 142px
		height 142px
		border 1px solid #d4d4d4
		border-radius 1px
	.lot-link
		color #336699
		font-size 12px
		text-decoration underline
		display block
		margin 0 auto
		width 143px
		height 34px
		text-align left
		overflow hidden
	.lot-link:hover
		text-decoration none
	.lot-wrapper.forth
		margin-right 0
	.lot-wrapper:before
		content ''
		position absolute
		top 0
		left 0
		right 0
		bottom 0
		border-radius 3px
		background linear-gradient(top,#d4d4d4 0%, #c8c8c8 100%)
		z-index -2
	.lot-wrapper:after
		content ''
		position absolute
		top 1px
		left 1px
		right 1px
		bottom 2px
		border-radius 3px
		border-bottom 1px solid #d1d1d1
		background-color #f8f8f8
		box-shadow 0 1px 0 #f1f1f1
		z-index -1
	.lots-finish
		height 2px
		border-top 1px solid #ededed
		border-bottom 1px solid #eaeaea
	.latest-exchanges-header
		color #336699
		font 15px arial
		margin-top 6px
		margin-left 24px
	.exchange-wrapper
		float left
		width 232px
		height 112px
		border 2px solid #dedede
		margin-right 26px
		margin-bottom 12px
		border-radius 4px
		box-shadow 0 1px 4px rgba(0,0,0,.2)
		background-color #f8f8f8
		.lot1-wrapper
		.lot2-wrapper
			width 78px
			height 110px
			padding-top 12px
			.ex-lot-image-link
				display block
				.ex-lot-image
					width 79px
					height 59px
					border 1px solid #e0e0e0
			.ex-lot-link
				display block
				color #336699
				font-size 10px
				text-decoration underline
				width 78px
				height 32px
				text-align left
				overflow hidden
				padding 0
		.lot1-wrapper
			float left
			margin-left 18px
			position relative
		.lot1-wrapper:after
			content ''
			width 15px
			height 13px
			position absolute
			top 36px
			right -26px
			background-image: url("data:image/png;base64,")
		.lot2-wrapper
			float right
			margin-right 18px
	.exchange-wrapper.third
		margin-right 0
/* **********************************
 *             Footer
 * *********************************/
.footer-info-strip
	background-image url(/i/footer_bg_grey.jpg)
	min-height 300px
	box-shadow inset 0 1px 3px rgba(0,0,0,.2)
.footer-info-wrapper
	width 980px
	margin 0 auto
	padding-top 12px
	.footer-info-article
		float left
		width 785px
		padding-right 24px
	.footer-news
		float right
		width 195px
	.info-header
		color #336699
		font-size 18px
	.info-article
		color #727272
		font 13px arial
		line-height 1.7em
	.news-wrapper
		margin-bottom 38px
		position relative
		.news-date
			color #7a7a7a
		.news-title-link
			color main_header_text_color
			text-decoration underline
		.news-title-link:hover
			text-decoration none
		.news-text
			color #333
			width 195px
			height 60px
			overflow hidden
	.news-wrapper.first:after
		content ''
		position absolute
		border-top 1px solid #c6c6c6
		border-bottom 1px solid #fff
		height 2px
		width 195px
		bottom -22px
.footer-copy-strip
	background-image url(/i/footer_bg_blue.jpg)
	height 170px
	box-shadow inset 0 5px 10px rgba(23,55,93,.8)
.footer-copy-wrapper
	width 980px
	margin 0 auto
	padding-top 24px
.copy
	float left
	width 280px
	.copyright
		color #529be5
		font-size 12px
	.email
		text-decoration none
		color #fff
.footer-categories
	float left
	width 460px
	.cats
		list-style none
		display inline-block
		padding 0
		margin-right 24px
		.fcat
			color #d7dbe1
			text-decoration underline
			line-height 2em
		.fcat:hover
			text-decoration none
.vgolos
	float left
	width 240px
	.studio-header
		color #529be5
		margin-top 0
		font-size 16px
	.studio-link
		color #d7dbe1
		text-decoration underline
	.studio-link:hover
		text-decoration none
